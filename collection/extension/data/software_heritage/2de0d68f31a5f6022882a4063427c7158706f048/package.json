  "scripts": {
    "prettier": "prettier --no-bracket-spacing --single-quote --write src/*.js src/*/*.js src/*/*/*.js src/*/*/*/*.js src/*/*/*/*/*.js src/*/*/*/*/*/*.js e2e/cypress/*/*.js",
    "build": "react-scripts build",
    "coverage": ". ./current.env && nyc --reporter=lcov _mocha --exit -- src/server/*_test.js src/lib/*_test.js src/lib/services/*/*_test.js src/acceptance/*_test.js",
    "init-storage": ". ./current.env && ./tools/create-storage-type.js",
    "db-migrate": ". ./current.env && knex --knexfile src/server/knexfile.js --cwd . migrate:latest",
    "fetch-metakompas": ". ./current.env && ./tools/fetch-data.js ",
    "inject-metakompas": ". ./current.env && ./tools/put-data.js",
    "fetch-inject-metakompas": "npm run fetch-metakompas && npm run inject-metakompas",
    "dbase": "docker-compose -f docker-compose-cypress.yml run --service-ports database",
    "lint-js": "eslint src",
    "lint:checkstyle": "eslint src -o report.xml -f checkstyle",
    "postinstall": "./tools/setup-node-env.sh",
    "start": "run-p start-backend start-frontend",
    "start-backend": ". ./current.env && touch ./storage_type_id.env && . ./storage_type_id.env && nodemon --exec node --no-stdin src/server/main.js --watch src/",
    "start-frontend": ". ./current.env && react-scripts start",
    "test": "run-s test-frontend test-units lint-js",
    "test-acceptance": ". ./env/integration.env && mocha --exit src/acceptance/*_test.js --timeout 5000",
    "test-frontend": "CI=true  react-scripts test --colors ",
    "test-frontend:watch": "react-scripts test ",
    "test-full": "run-s test test-no-elvis",
    "test-no-db": ". ./current.env && mocha --exit src/acceptance/*_no-db.js",
    "test-no-elvis": "run-s test-no-db test-acceptance test-frontend",
    "test-units": "mocha --exit src/server/*_test.js src/lib/*_test.js src/lib/services/*/*_test.js",
    "cy:run": "npx cypress run --project ./e2e",
    "cy:open": "npx cypress@latest open --project ./e2e",
    "version": "auto-changelog --package --commit-limit 0 && git add CHANGELOG.md"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version",
      "last 2 edge version",
      "IE 10"
    ]
  }
}
