{
  "name": "iD",
  "version": "2.12.1",
  "description": "A friendly editor for OpenStreetMap",
  "main": "iD.js",
  "repository": "openstreetmap/iD",
  "keywords": [
    "editor",
    "openstreetmap"
  ],
  "license": "ISC",
  "scripts": {
    "all": "npm-run-all -s clean build dist",
    "build": "webpack --env.NODE_ENV=prod",
    "build:analyze": "webpack --env.NODE_ENV=analyze",
    "clean": "shx rm -f dist/*.js dist/*.map dist/*.css dist/img/*.svg",
    "dist": "npm-run-all -p dist:**",
    "dist:mapillary": "shx mkdir -p dist/mapillary-js && shx cp -R node_modules/mapillary-js/dist/* dist/mapillary-js/",
    "dist:pannellum": "shx mkdir -p dist/pannellum-streetside && shx cp -R node_modules/pannellum/build/* dist/pannellum-streetside/",
    "dist:svg:id": "svg-sprite --symbol --symbol-dest . --shape-id-generator \"iD-%s\" --symbol-sprite img/iD-sprite.svg 'svg/iD-sprite/**/*.svg'",
    "dist:svg:community": "svg-sprite --symbol --symbol-dest . --shape-id-generator \"community-%s\" --symbol-sprite img/community-sprite.svg node_modules/osm-community-index/dist/img/*.svg",
    "dist:svg:fa": "svg-sprite --symbol --symbol-dest . --symbol-sprite img/fa-sprite.svg svg/fontawesome/*.svg",
    "dist:svg:maki": "svg-sprite --symbol --symbol-dest . --shape-id-generator \"maki-%s\" --symbol-sprite img/maki-sprite.svg node_modules/@mapbox/maki/icons/*.svg",
    "dist:svg:mapillary": "svg-sprite --symbol --symbol-dest . --symbol-sprite img/mapillary-sprite.svg node_modules/mapillary_sprite_source/package_signs/*.svg",
    "dist:svg:temaki": "svg-sprite --symbol --symbol-dest . --shape-id-generator \"temaki-%s\" --symbol-sprite img/temaki-sprite.svg node_modules/temaki/icons/*.svg",
    "imagery": "node data/update_imagery",
    "lint": "eslint *.js test/spec modules",
    "lintfix": "eslint --fix *.js test/spec modules",
    "production": "npm-run-all -p dist:** && webpack --env.NODE_ENV=prod",
    "start": "webpack-dev-server --env.NODE_ENV=dev",
    "test": "npm-run-all -s lint build test:**",
    "test:hoot": "karma start",
    "testgui:hoot": "karma start --single-run --browsers Chrome",
    "test:phantom": "phantomjs --web-security=no node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js test/index.html spec",
    "translations": "node data/update_locales",
    "buildinfo": "UI_VERSION=`git describe --tags` && echo '{\"name\": \"Hootenanny iD Editor\", \"version\": \"'$UI_VERSION'\", \"user\": \"webpack\"}' > modules/Hoot/config/buildInfo.json"
  },
  "dependencies": {
    "@mapbox/sexagesimal": "1.1.0",
    "@mapbox/togeojson": "0.16.0",
    "@mapbox/vector-tile": "^1.3.1",
    "@turf/bbox-clip": "^6.0.0",
    "axios": "^0.18.0",
    "diacritics": "1.3.0",
    "express": "^4.16.3",
    "fast-json-stable-stringify": "2.0.0",
    "file-saver": "^2.0.1",
    "lodash": "^4.17.11",
    "lodash-es": "4.17.11",
    "marked": "0.5.2",
    "martinez-polygon-clipping": "0.5.0",
    "material-icons": "^0.3.1",
    "node-diff3": "1.0.0",
    "osm-auth": "1.0.2",
    "pannellum": "2.4.1",
    "progressbar.js": "^1.0.1",
    "q": "1.5.1",
    "rbush": "2.0.2",
    "which-polygon": "2.2.0",
    "wmf-sitematrix": "0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "7.0.0-beta.55",
    "@fortawesome/fontawesome-svg-core": "~1.2.8",
    "@fortawesome/free-brands-svg-icons": "~5.5.0",
    "@fortawesome/free-regular-svg-icons": "~5.5.0",
    "@fortawesome/free-solid-svg-icons": "~5.5.0",
    "@mapbox/maki": "^5.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.0",
    "colors": "^1.1.2",
    "concat-files": "^0.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "d3": "4.13.0",
    "ecstatic": "^3.0.0",
    "editor-layer-index": "osmlab/editor-layer-index.git#gh-pages",
    "eslint": "^5.0.0",
    "esm": "~3.1.0",
    "express-http-proxy": "^1.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "gaze": "^1.1.1",
    "glob": "^7.1.0",
    "happen": "^0.3.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "js-yaml": "^3.13.1",
    "json-stringify-pretty-compact": "^1.1.0",
    "jsonschema": "^1.1.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-express-http-server": "0.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-scss-preprocessor": "^3.0.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "mapillary-js": "2.14.0",
    "mapillary_sprite_source": "^1.7.0",
    "mini-css-extract-plugin": "^0.4.4",
    "minimist": "^1.2.0",
    "mocha": "^5.0.0",
    "mocha-phantomjs-core": "^2.1.0",
    "name-suggestion-index": "1.0.1",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "osm-community-index": "0.5.0",
    "phantomjs-prebuilt": "~2.1.11",
    "request": "^2.88.0",
    "rollup": "~0.67.1",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-includepaths": "~0.2.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-scss": "^0.4.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.0",
    "shx": "^0.3.0",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.1.0",
    "smash": "0.0",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "svg-sprite": "1.5.0",
    "temaki": "1.0.0",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.22.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "xml2js": "^0.4.17",
    "xmlbuilder": "^10.0.0"
  },
  "greenkeeper": {
    "label": "chore-greenkeeper",
    "ignore": [
      "d3"
    ]
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
