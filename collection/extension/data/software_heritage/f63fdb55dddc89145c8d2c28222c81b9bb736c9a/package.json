    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "~16.11.0",
    "source-map-explorer": "^2.4.0",
    "svgo": "^1.3.2"
  },
  "browserslist": [
    ">0.2%",
    "Firefox ESR",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
